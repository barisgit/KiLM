{
  "name": "kilm-docs",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "sync-docs": "tsx scripts/sync-embedded-docs.ts sync",
    "sync-docs:watch": "tsx scripts/sync-embedded-docs.ts watch",
    "sync-docs:clean": "tsx scripts/sync-embedded-docs.ts clean",
    "dev": "concurrently \"pnpm sync-docs:watch\" \"astro dev\" --names \"sync,astro\" --prefix-colors \"blue,green\"",
    "start": "pnpm dev",
    "build": "pnpm sync-docs && astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/starlight": "^0.33.2",
    "@astrojs/starlight-tailwind": "^4.0.0",
    "@tailwindcss/vite": "^4.1.4",
    "astro": "^5.6.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.4"
  },
  "devDependencies": {
    "sharp": "^0.32.6",
    "tsx": "^4.19.3",
    "chokidar": "^4.0.2",
    "@types/node": "^22.10.3",
    "concurrently": "^9.1.0"
  }
}
